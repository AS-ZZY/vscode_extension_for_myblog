{
	"name": "blog",
	"displayName": "blogforzzy",
	"description": "blog for zzy",
	"version": "0.0.1",
	"publisher":"aszzy",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:blog.uploadPaper",
		"onCommand:blog.uploadImg"
	],
	"main": "./src/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "blogforzzy",
			"properties": {
				"vscodePluginDemo.username": {
					"type": "string",
					"default": "aszzy",
					"description": "用户名："
				},
				"vscodePluginDemo.url": {
					"type": "string",
					"default": "http://127.0.0.1:5000/api/",
					"description": "地址："
				},
				"vscodePluginDemo.token": {
					"type": "string",
					"default": "",
					"description": "token："
				}
			}
		},
		"commands": [
			{
				"command": "blog.uploadPaper",
				"title": "上传文章"
			},
			{
				"command": "blog.uploadImg",
				"title": "上传图片"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorLangId == markdown",
					"command": "blog.uploadPaper",
					"alt": "blog.uploadPaper",
					"group": "navigation"
				},
				{
					"when": "editorLangId == markdown",
					"command": "blog.uploadImg",
					"alt": "blog.uploadImg",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.19.2"
	}
}
